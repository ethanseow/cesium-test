{
    "configurations": [

        {
            "name": "Attach to server.js",
            "port": 9229,
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",

        },
        {
            "name": "Launch server.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js",
            "type": "node"
        },
        {
            "name": "Launch nodemon server.js",
            "request": "launch",
            "runtimeExecutable": "nodemon",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js",
            "type": "node"
        },
        {
            "name": "Launch nodemon server-test.js",
            "request": "launch",
            "runtimeExecutable": "nodemon",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server-test.js",
            "type": "node"
        }
        
    ]
}